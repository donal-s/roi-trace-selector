{
  "name": "react-trace-selector",
  "version": "0.21.2",
  "private": true,
  "dependencies": {
    "@babel/eslint-parser": "^7.25.8",
    "@babel/runtime": "^7.25.7",
    "@reduxjs/toolkit": "^2.2.8",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0"
  },
  "scripts": {
    "start": "npx webpack serve --config webpack.development.config.js",
    "build": "npx genversion src/version.js && npx webpack --config webpack.production.config.js",
    "test": "NODE_ENV=test BABEL_ENV=test jest --watch",
    "test:ci": "NODE_ENV=test BABEL_ENV=test jest",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src assembly",
    "asbuild:debug": "asc assembly/index.ts --exportRuntime --target debug",
    "asbuild:release": "asc assembly/index.ts --exportRuntime --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@eslint/js": "^9.12.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/eslint__js": "^8.42.3",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.11",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "assemblyscript": "^0.27.30",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.20.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-webpack-plugin": "^4.2.0",
    "genversion": "^3.0.2",
    "globals": "^15.11.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "mini-css-extract-plugin": "^2.5.2",
    "prettier": "3.3.3",
    "raw-loader": "^4.0.2",
    "react-dev-utils": "^12.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "resolve": "^1.21.1",
    "resolve-url-loader": "^5.0.0",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.2.6",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.8.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-manifest-plugin": "^5.0.0",
    "workbox-webpack-plugin": "^7.1.0"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.test\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.csv$": "<rootDir>/config/jest/rawTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
