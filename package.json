{
  "name": "react-trace-selector",
  "version": "0.21.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.6.0",
    "chart.js": "^3.3.2",
    "chartjs-plugin-annotation": "^1.0.2",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.4",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "npx webpack serve --config webpack.development.config.js",
    "build": "npx genversion src/version.js && npx webpack --config webpack.production.config.js",
    "test": "NODE_ENV=test BABEL_ENV=test jest --watch",
    "test:ci": "NODE_ENV=test BABEL_ENV=test jest",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "react-app",
      "plugin:jest/all"
    ],
    "plugins": [
      "jest"
    ],
    "rules": {
      "jest/prefer-expect-assertions": "off",
      "jest/no-hooks": "off",
      "jest/prefer-spy-on": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "camelcase": "^6.2.0",
    "copy-webpack-plugin": "^9.0.0",
    "core-js": "^3.15.1",
    "css-loader": "^5.1.0",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "eslint": "^7.21.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "genversion": "^3.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.4",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom": "^27.0.3",
    "jest-raw-loader": "^1.0.1",
    "jest-watch-typeahead": "^0.6.4",
    "mini-css-extract-plugin": "^1.3.9",
    "raw-loader": "^4.0.2",
    "react-dev-utils": "^11.0.3",
    "redux-mock-store": "^1.5.4",
    "resize-observer-polyfill": "^1.5.1",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.0.0",
    "workbox-webpack-plugin": "^6.1.1"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.csv$": "jest-raw-loader",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
